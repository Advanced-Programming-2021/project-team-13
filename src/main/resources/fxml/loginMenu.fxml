<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.allMenu.LoginView">
   <Button fx:id="loginButton" layoutX="224.0" layoutY="200.0" mnemonicParsing="false" onAction="#goToMainMenu"
           prefHeight="26.0" prefWidth="129.0" text="login"/>
   <TextField fx:id="usernameField" layoutX="214.0" layoutY="83.0" promptText="username"/>
   <PasswordField fx:id="passwordField" layoutX="214.0" layoutY="135.0" promptText="password"/>
   <Button fx:id="registerButton" layoutX="262.0" layoutY="284.0" mnemonicParsing="false" onAction="#goToRegisterMenu"
           text="register"/>
   <Button fx:id="exitButton" layoutX="550.0" layoutY="360.0" mnemonicParsing="false" onAction="#exit" text="exit"/>
</AnchorPane>
